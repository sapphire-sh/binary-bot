{
  "name": "binary_bot",
  "version": "1.0.0",
  "description": "@binary_bot",
  "main": "./src/main.js",
  "scripts": {
    "_build": "webpack",
    "build:dev": "cross-env NODE_ENV=development npm run _build",
    "build:prod": "cross-env NODE_ENV=production npm run _build",
    "build": "npm run build:prod",
    "start:dev": "nodemon ./dist/index.js --watch ./dist/index.js",
    "start:prod": "node ./dist/index.js",
    "start": "npm run start:prod",
    "test": "jest",
    "watch:build": "npm run build:dev -- --watch",
    "watch:test": "npm run test -- --watch",
    "watch": "concurrently \"npm run watch:build\" \"npm run watch:test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sapphiredev/binary_bot.git"
  },
  "keywords": [
    "twitter",
    "bot"
  ],
  "author": "sapphire",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sapphiredev/binary_bot/issues"
  },
  "homepage": "https://github.com/sapphiredev/binary_bot#readme",
  "dependencies": {
    "ioredis": "^4.9.5",
    "node-schedule": "^1.3.2",
    "twit": "^2.2.11"
  },
  "devDependencies": {
    "@types/ioredis": "^4.0.11",
    "@types/ioredis-mock": "github:5minlab/types-ioredis-mock#dev",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.3",
    "@types/node-schedule": "^1.2.3",
    "@types/twit": "^2.2.22",
    "cross-env": "^5.2.0",
    "ioredis-mock": "^4.12.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2"
  }
}
